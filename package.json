    "css-modules-require-hook": "4.0.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "history": "2.1.0",
    "jade": "1.11.0",
    "morgan": "1.7.0",
    "postcss-loader": "0.8.2",
    "postcss-nested": "1.0.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "redux": "3.4.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0"
