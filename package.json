{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack -c --progress",
    "lint": "eslint --ignore-path .gitignore . && stylelint client/**/*.css",
    "prebuild": "rm -rf build/*",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server",
    "start-dev": "nodemon server -w server",
    "test": "NODE_ENV=test mocha",
    "test-watch": "NODE_ENV=test mocha -R min -w"
  },
  "devDependencies": {
    "babel-eslint": "6.0.2",
    "eslint-config-richardkall": "2.8.0",
    "eslint-plugin-react": "5.0.1",
    "eslint": "2.8.0",
    "expect-jsx": "2.5.1",
    "expect": "1.18.0",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "3.0.0-alpha.12",
    "stylelint-config-richardkall": "6.0.1",
    "stylelint": "6.0.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.4.0",
    "autoprefixer": "6.3.6",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
